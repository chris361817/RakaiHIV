Cell[CellGroupData[{

Cell[TextData[{
 "TransmitTab[patients_, regions_, alphas_]\n",
 StyleBox["For a list of patients, gene regions and alphas, this returns a \
table where each column gives {patient, region, timestamp, alpha, \
mean_mutatations_transmitted, mean_Smutatations_transmitted, \
mean_NSmutatations_transmitted}. alpha=0 corresponds to no selection at \
transmission, and therefore also reflects the mean number of mutations in an \
individual before transmission.", "Text"]
}], "Subsection",
 CellChangeTimes->{{3.7161158786081*^9, 3.7161160196933928`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"TransmitTab", "[", 
    RowBox[{"patients_", ",", "regions_", ",", "alphas_"}], "]"}], ":=", 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"TransmitTab", "[", 
     RowBox[{"patients", ",", "regions", ",", "alphas"}], "]"}], "=", 
    RowBox[{"Block", "[", 
     RowBox[{
      RowBox[{"{", "tab", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"tab", "=", 
        RowBox[{"{", 
         RowBox[{"{", 
          RowBox[{
          "\"\<patient\>\"", ",", "\"\<region\>\"", ",", "\"\<time\>\"", ",", 
           "\"\<alpha\>\"", ",", "\"\<mean_transmitted\>\"", ",", 
           "\"\<mean_S_transmitted\>\"", ",", "\"\<mean_NS_transmitted\>\""}],
           "}"}], "}"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"For", "[", 
        RowBox[{
         RowBox[{"pp", "=", "1"}], ",", 
         RowBox[{"pp", "\[LessEqual]", 
          RowBox[{"Length", "[", "patients", "]"}]}], ",", 
         RowBox[{"pp", "++"}], ",", "\[IndentingNewLine]", 
         RowBox[{"For", "[", 
          RowBox[{
           RowBox[{"rr", "=", "1"}], ",", 
           RowBox[{"rr", "\[LessEqual]", "2"}], ",", 
           RowBox[{"rr", "++"}], ",", "\[IndentingNewLine]", 
           RowBox[{"For", "[", 
            RowBox[{
             RowBox[{"ss", "=", "1"}], ",", 
             RowBox[{"ss", "\[LessEqual]", "4"}], ",", 
             RowBox[{"ss", "++"}], ",", "\[IndentingNewLine]", 
             RowBox[{"tab", "=", 
              RowBox[{"Join", "[", 
               RowBox[{"tab", ",", 
                RowBox[{"Transmitted", "[", 
                 RowBox[{
                  RowBox[{"patients", "[", 
                   RowBox[{"[", "pp", "]"}], "]"}], ",", 
                  RowBox[{"regions", "[", 
                   RowBox[{"[", "rr", "]"}], "]"}], ",", 
                  RowBox[{"alphas", "[", 
                   RowBox[{"[", "ss", "]"}], "]"}]}], "]"}]}], "]"}]}]}], 
            "]"}]}], "]"}]}], "]"}], ";", "\[IndentingNewLine]", "tab"}]}], 
     "]"}]}]}], "\[IndentingNewLine]"}]], "Input",
