     RowBox[{"seqData", "=", 
      RowBox[{"Import", "[", 
       RowBox[{
        RowBox[{
        "patient", "<>", "\"\<_\>\"", "<>", "region", "<>", 
         "\"\<.fasta\>\""}], ",", 
        RowBox[{"{", 
         RowBox[{"\"\<FASTA\>\"", ",", "\"\<Data\>\""}], "}"}]}], "]"}]}], 
     ";", 
     RowBox[{"(*", 
      RowBox[{
      "import", " ", "all", " ", "the", " ", "seqeunces", " ", "for", " ", 
       "each", " ", "individual", " ", "patient"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"names", "=", 
      RowBox[{"seqData", "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", 
