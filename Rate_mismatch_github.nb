Cell[CellGroupData[{

Cell[TextData[{
 "Transmitted[patient_, region_, alpha_]\n",
 StyleBox["returns the number of transmitted mutations for all, synonymous \
and nonsynonymous mutations from the ancestral sequences, assuming fitness \
depends on number of all mutations for each sequence. When alpha = 0, there \
is no selection at transmission", "Text"]
}], "Subsection",
 CellChangeTimes->{{3.7161097610300007`*^9, 3.7161097818861856`*^9}, {
  3.716109850116808*^9, 3.716109937326973*^9}, {3.7161149356781063`*^9, 
  3.716114950841457*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "this", " ", "gives", " ", "transmitted", " ", "mutations", " ", 
    "assuming", " ", "fitness", " ", "depends", " ", "on", " ", "number", " ",
     "of", " ", "all", " ", "mutations", " ", "for", " ", "each", " ", 
    "sequence"}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Transmitted", "[", 
    RowBox[{"patient_", ",", "region_", ",", "alpha_"}], "]"}], ":=", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "distanceTab", ",", "stamps", ",", "nMutMax", ",", "fitness", ",", 
       "fitnessTab", ",", "sumTrans", ",", "totTrans", ",", "freqTrans", ",", 
       "meanTrans"}], "\[IndentingNewLine]", "\[IndentingNewLine]", "}"}], 
     ",", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"SetDirectory", "[", "]"}], ";", 
      RowBox[{"SetDirectory", "[", "mismatchDir", "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"distanceTab", "=", 
       RowBox[{
        RowBox[{"Import", "[", 
         RowBox[{
         "patient", "<>", "\"\<_\>\"", "<>", "region", "<>", 
          "\"\<_distance_from_ancestor.csv\>\""}], "]"}], "[", 
        RowBox[{"[", 
         RowBox[{"2", ";;"}], "]"}], "]"}]}], ";", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{
        "this", " ", "imports", " ", "a", " ", "table", " ", "where", " ", 
         "each", " ", "row", " ", "represents", " ", "a", " ", "sequence", 
         " ", "and", " ", "each", " ", "column", " ", "gives", " ", "the", 
         " ", "time", " ", "stamp"}], ",", " ", 
        RowBox[{
        "number", " ", "of", " ", "copies", " ", "of", " ", "that", " ", 
         "sequence"}], ",", " ", 
        RowBox[{"total", " ", "number", " ", "of", " ", "mutations"}], ",", 
        " ", 
        RowBox[{"number", " ", "of", " ", "synonymous", " ", "mutations"}], 
        ",", " ", 
        RowBox[{
        "and", " ", "number", " ", "of", " ", "nonsynonymous", " ", 
         "mutations"}]}], "*)"}], "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"stamps", "=", 
       RowBox[{"Sort", "[", 
        RowBox[{
         RowBox[{"DeleteDuplicates", "[", 
          RowBox[{"distanceTab", "[", 
           RowBox[{"[", 
            RowBox[{";;", ",", "1"}], "]"}], "]"}], "]"}], ",", 
         RowBox[{
          RowBox[{"#2", ">", "#1"}], "&"}]}], "]"}]}], ";", 
      RowBox[{"(*", 
       RowBox[{
       "gives", " ", "a", " ", "list", " ", "of", " ", "all", " ", "of", " ", 
        "the", " ", "sampled", " ", "timepoints"}], "*)"}], 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"nMutMax", "=", 
       RowBox[{"Max", "[", 
        RowBox[{"distanceTab", "[", 
         RowBox[{"[", 
          RowBox[{";;", ",", "3"}], "]"}], "]"}], "]"}]}], ";", 
      RowBox[{"(*", 
       RowBox[{
       "the", " ", "maximum", " ", "number", " ", "of", " ", "mutations", " ",
         "for", " ", "this", " ", "patient"}], "*)"}], "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"fitness", "=", 
       RowBox[{"Exp", "[", 
        RowBox[{
         RowBox[{"-", "alpha"}], " ", 
         RowBox[{"distanceTab", "[", 
          RowBox[{"[", 
           RowBox[{";;", ",", "3"}], "]"}], "]"}]}], "]"}]}], ";", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{"for", " ", "each", " ", "sequence"}], ",", " ", 
        RowBox[{"this", " ", "gives", " ", "its", " ", "fitness"}]}], "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"fitnessTab", "=", 
       RowBox[{
        RowBox[{"Join", "[", 
         RowBox[{
          RowBox[{"distanceTab", "//", "Transpose"}], ",", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"distanceTab", "[", 
             RowBox[{"[", 
              RowBox[{";;", ",", "2"}], "]"}], "]"}], "fitness"}], "}"}]}], 
         "]"}], "//", "Transpose"}]}], ";", 
      RowBox[{"(*", 
       RowBox[{
       "this", " ", "adds", " ", "an", " ", "extra", " ", "column", " ", "to",
         " ", "distanceTab", " ", "giving", " ", "the", " ", "adjusted", " ", 
        "number", " ", "of", " ", "sequences", " ", "based", " ", "on", " ", 
        "the", " ", "fitness", " ", "of", " ", "that", " ", "sequence"}], 
       "*)"}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"sumTrans", "=", 
       RowBox[{
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"Total", "[", 
           RowBox[{
            RowBox[{"fitnessTab", "[", 
             RowBox[{"[", 
              RowBox[{
               RowBox[{"Intersection", "[", 
                RowBox[{
                 RowBox[{"Position", "[", 
                  RowBox[{
                   RowBox[{"fitnessTab", "[", 
                    RowBox[{"[", 
                    RowBox[{";;", ",", "1"}], "]"}], "]"}], ",", 
                   RowBox[{"stamps", "[", 
                    RowBox[{"[", "tt", "]"}], "]"}]}], "]"}], ",", 
                 RowBox[{"Position", "[", 
                  RowBox[{
                   RowBox[{"fitnessTab", "[", 
                    RowBox[{"[", 
                    RowBox[{";;", ",", "ss"}], "]"}], "]"}], ",", "nn"}], 
                  "]"}]}], "]"}], "//", "Flatten"}], "]"}], "]"}], "[", 
            RowBox[{"[", 
             RowBox[{";;", ",", "6"}], "]"}], "]"}], "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"tt", ",", 
            RowBox[{"Length", "[", "stamps", "]"}]}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"nn", ",", "0", ",", "nMutMax"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"ss", ",", "3", ",", "5"}], "}"}]}], "]"}], "//", "N"}]}], 
      ";", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{
         RowBox[{"an", " ", "nstamp", " ", "x", " ", "nMutMax"}], "+", 
         RowBox[{
         "1", " ", "x", " ", "3", " ", "table", " ", "giving", " ", "for", 
          " ", "each", " ", "timestamp", " ", "the", " ", "ADJUSTED", " ", 
          "number", " ", "of", " ", "sequences", " ", "with", " ", "0"}]}], 
        ",", "..", ",", " ", 
        RowBox[{
        "nmuttotal", " ", "mutations", " ", "for", " ", "all", " ", 
         "mutations"}], ",", " ", 
        RowBox[{"synonmymous", " ", "and", " ", "nonsynonymous"}]}], "*)"}], 
      "\[IndentingNewLine]", "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"totTrans", "=", 
       RowBox[{"Total", "[", 
        RowBox[{"sumTrans", ",", 
         RowBox[{"{", "2", "}"}]}], "]"}]}], ";", 
      RowBox[{"(*", 
       RowBox[{
       "gives", " ", "the", " ", "total", " ", "number", " ", "of", " ", 
        "sequences", " ", 
        RowBox[{"(", 
         RowBox[{"all", ",", " ", "synon", ",", " ", "nonsynon"}], ")"}], " ",
         "for", " ", "each", " ", "timepoint"}], "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"freqTrans", "=", 
       RowBox[{"sumTrans", "/", 
        RowBox[{
         RowBox[{"Total", "[", 
          RowBox[{"sumTrans", ",", 
           RowBox[{"{", "2", "}"}]}], "]"}], "[", 
         RowBox[{"[", 
          RowBox[{";;", ",", "1"}], "]"}], "]"}]}]}], ";", 
      RowBox[{"(*", 
       RowBox[{"like", " ", "sumwithin", " ", "by", " ", "frequencies"}], 
       "*)"}], "\[IndentingNewLine]", 
      RowBox[{"meanTrans", "=", 
       RowBox[{
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"Total", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"freqTrans", "[", 
              RowBox[{"[", "ii", "]"}], "]"}], 
             RowBox[{"Range", "[", 
              RowBox[{"0", ",", "nMutMax"}], "]"}]}], ",", "1"}], "]"}], ",", 
          
          RowBox[{"{", 
           RowBox[{"ii", ",", 
            RowBox[{"Length", "[", "stamps", "]"}]}], "}"}]}], "]"}], "//", 
        "N"}]}], ";", 
      RowBox[{"(*", 
       RowBox[{
       "gives", " ", "the", " ", "mean", " ", "number", " ", "of", " ", 
        "mutations", " ", "for", " ", "each", " ", "timepoint"}], "*)"}], 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Join", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"patient", ",", "region", ",", 
              RowBox[{"stamps", "[", 
               RowBox[{"[", "ss", "]"}], "]"}], ",", "alpha"}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"ss", ",", 
              RowBox[{"Length", "[", "stamps", "]"}]}], "}"}]}], "]"}], "//", 
          "Transpose"}], ",", 
         RowBox[{"meanTrans", "//", "Transpose"}]}], "]"}], "//", 
       "Transpose"}]}]}], " ", 
    RowBox[{"(*", 
     RowBox[{
      RowBox[{
      "gives", " ", "and", " ", "nstamp", " ", "x", " ", "7", " ", "matrix"}],
       ",", " ", 
      RowBox[{
      "where", " ", "each", " ", "columb", " ", "gives", " ", "the", " ", 
       "patient"}], ",", " ", "region", ",", " ", 
      RowBox[{"time", " ", "stamp"}], ",", " ", "alpha", ",", " ", 
      RowBox[{
      "mean", " ", "number", " ", "of", " ", "transmitted", " ", "all"}], ",",
       " ", 
      RowBox[{"S", " ", "and", " ", "NS", " ", "mutations"}]}], "*)"}], 
    "\[IndentingNewLine]", "]"}]}]}]], "Input",
