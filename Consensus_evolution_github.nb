     RowBox[{"temp", "=", 
      RowBox[{"sequences", "[", 
       RowBox[{"[", 
        RowBox[{"All", ",", "i"}], "]"}], "]"}]}], ";", 
     RowBox[{"(*", 
      RowBox[{
