        RowBox[{
        "and", " ", "therefore", " ", "reflects", " ", "the", " ", "mean", 
         " ", "number", " ", "of", " ", "mutations", " ", "in", " ", "an", 
         " ", "individual", " ", "before", " ", "transmission"}]}], "*)"}], 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"mutTab", "=", 
       RowBox[{"{", "}"}]}], ";", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{"for", " ", "the", " ", "given", " ", "timeinterval"}], ",", 
        " ", 
        RowBox[{
        "this", " ", "will", " ", "create", " ", "a", " ", "table", " ", 
         "where", " ", "each", " ", "row", " ", "represents", " ", "a", " ", 
         "patient"}], ",", " ", 
        RowBox[{
        "and", " ", "each", " ", "column", " ", "gives", " ", 
