  3.716105896888723*^9, 3.716105902422963*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"SeqDat", "[", 
   RowBox[{"ans_", ",", "seq_", ",", "name_"}], "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "snps", ",", "nMuts", ",", "ansIndex", ",", "snpCodon", ",", "ansAA", 
      ",", "seqAA", ",", "nSynon"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"Off", "[", 
       StyleBox[
        RowBox[{"Part", "::", "partw"}], "MessageName"], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Off", "[", 
       StyleBox[
        RowBox[{"StringJoin", "::", "string"}], "MessageName"], "]"}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"snps", "=", 
       RowBox[{"Map", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"SNP", "[", 
           RowBox[{"#", ",", "seq"}], "]"}], "&"}], ",", "ans"}], "]"}]}], 
      ";"}], ";", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{
       "the", " ", "positions", " ", "where", " ", "each", " ", "ancestral", 
        " ", "consensus", " ", "sequence"}], " ", "\[NotEqual]", " ", 
       "seqeunce"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"nMuts", "=", 
      RowBox[{"Min", "[", 
       RowBox[{"Map", "[", 
        RowBox[{"Length", ",", "snps"}], "]"}], "]"}]}], ";", 
     RowBox[{"(*", 
      RowBox[{
      "the", " ", "number", " ", "of", " ", "differences", " ", "between", 
       " ", "seqeunce", " ", "and", " ", "the", " ", "closest", " ", 
       "ancestor"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"ansIndex", "=", 
      RowBox[{
       RowBox[{"Position", "[", 
        RowBox[{
         RowBox[{"Map", "[", 
          RowBox[{"Length", ",", "snps"}], "]"}], ",", "nMuts"}], "]"}], "[", 
       
       RowBox[{"[", 
        RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ";", 
     RowBox[{"(*", 
      RowBox[{
      "the", " ", "index", " ", "of", " ", "the", " ", "ancestral", " ", 
       "sequence", " ", "to", " ", "which", " ", "the", " ", "sequence", " ", 
       "is", " ", "the", " ", 
       RowBox[{"closest", ".", "  ", "If"}], " ", "equal", " ", "distance", 
       " ", "to", " ", "more", " ", "than", " ", "one", " ", "ancestral", " ",
        "sequence", " ", "it", " ", "will", " ", "give", " ", "the", " ", 
       "lowest", " ", "index"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"snpCodon", "=", 
      RowBox[{"CodonPos", "/@", 
       RowBox[{"snps", "[", 
        RowBox[{"[", "ansIndex", "]"}], "]"}]}]}], ";", 
     RowBox[{"(*", 
      RowBox[{
      "the", " ", "codon", " ", "triplets", " ", "where", " ", "there", " ", 
