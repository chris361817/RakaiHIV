        RowBox[{"Map", "[", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"ans", "[", 
             RowBox[{"[", "ansIndex", "]"}], "]"}], "[", 
            RowBox[{"[", "#", "]"}], "]"}], "&"}], ",", "snpCodon"}], 
         "]"}]}]}]}], ";", 
     RowBox[{"(*", 
      RowBox[{
      "the", " ", "AA", " ", "in", " ", "the", " ", "consensus", " ", 
       "sequence", " ", "associated", " ", "with", " ", "each", " ", "SNP"}], 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"seqAA", "=", 
      RowBox[{"CodonToAA", "/@", 
       RowBox[{"StringJoin", "/@", 
        RowBox[{"Map", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"seq", "[", 
